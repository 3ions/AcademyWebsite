{
  "name": "academywebsite",
  "version": "1.0.1",
  "description": "Website for JSRR Academy",
  "main": "server.js",
  "scripts": {
    "build": "cd jsrracademy && npm run build",
    "install-client": "cd jsrracademy && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd jsrracademy && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "author": "3ions",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.11",
    "nodemailer": "^6.4.14",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-router": "^5.2.0",
    "validator": "^13.1.17"
  }
}
